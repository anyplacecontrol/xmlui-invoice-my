{
  "files.associations": {
    "*.xmlui": "xmlui"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[xml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[xmlui]": {
    "editor.defaultFormatter": "xmlui.xmlui-vscode",
    "editor.formatOnSave": true
  },
  "prettier.printWidth": 200,
  "prettier.tabWidth": 2,
  "prettier.useTabs": false
}
